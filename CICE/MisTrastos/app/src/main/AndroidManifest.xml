<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ejemploporti.mistrastos" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.ejemploporti.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ejemploporti.activity.TrastoDetailActivity" >
        </activity>
        <activity
            android:name="com.ejemploporti.activity.NewTrastoActivity"
            android:windowSoftInputMode="stateAlwaysVisible|adjustResize">
        </activity>
    </application>

</manifest>
